{
    "s3_bucket": "database-parquet-files-ve",
    "s3_region": "eu-central-1",
    "retention_days": 7,
    "scrapers": [
        {
            "name": "ENTSOEImb15MinAT",
            "workers": 1,
            "task_generator_delay_ms": 2000,
            "url": "https://web-api.tp.entsoe.eu/api",
            "token": "e463828a-e195-450c-b9aa-3245f5bf9965",
            "sub_data_folder": "entsoe/imbalance/AT/15min"
        },
        {
            "name": "apg_imb_15min",
            "workers": 1,
            "task_generator_delay_ms": 200,
            "url": "https://transparency.apg.at/api/v1",
            "drz_mode": "OperationalOrSettlement",
            "value_column": "DRZ15M",
            "url_template": "DRZ/Data/English/PT15M/{from}/{to}?p_drzMode=OperationalOrSettlement",
            "time_offset_minutes": "-30",
            "sub_data_folder": "apg/imbalance/PT15M"
        },
        {
            "name": "apg_imb_5min",
            "workers": 1,
            "task_generator_delay_ms": 200,
            "url": "https://transparency.apg.at/api/v1",
            "drz_mode": "CurrentBalancingState5M",
            "url_template": "DRZ/Data/English/PT5M/{from}/{to}?p_drzMode=CurrentBalancingState5M",
            "time_offset_minutes": "-30",
            "value_column": "DRZ5M",
            "sub_data_folder": "apg/imbalance/PT5M"
        },
        {
            "name": "at_de_exchange",
            "workers": 1,
            "task_generator_delay_ms": 1000,
            "url": "https://transparency.apg.at/api/v1",
            "url_template": "ATC/Data/English/PT15M/{from}/{to}?p_border=AT<>DE",
            "value_columns": ["OfferedCapacityDEtoAT", "OfferedCapacityATtoDE"],
            "sub_data_folder": "apg/atc/at_de_exchange"
        },
        {
            "name": "at_cz_exchange",
            "workers": 1,
            "task_generator_delay_ms": 1000,
            "url": "https://transparency.apg.at/api/v1",
            "url_template": "ATC/Data/English/PT15M/{from}/{to}?p_border=AT<>CZ",
            "value_columns": ["OfferedCapacityCZtoAT", "OfferedCapacityATtoCZ"],
            "sub_data_folder": "apg/atc/at_cz_exchange"
        },
        {
            "name": "apg_imb_price_15min",
            "workers": 1,
            "task_generator_delay_ms": 1000,
            "url": "https://transparency.apg.at/api/v1",
            "value_column": "AEPrice",
            "url_template": "AE/Data/English/PT15M/{from}/{to}?p_aeMode=Default&p_aeTimeSeriesQuality=FirstValue",
            "sub_data_folder": "apg/imbalance/price/15min"
        },
        {
            "name": "balancing_energy_bids",
            "workers": 1,
            "task_generator_delay_ms": 5000,
            "url": "https://transparency.apg.at/api/v1",
            "url_template": "SET/Data/English/PT15M/{from}/{to}/Full?p_setMode=Overview",
            "sub_data_folder": "apg/balancing/bids",
            "is_balancing_bids": "true"
        }
    ]
}